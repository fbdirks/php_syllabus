<?php

// De functie. Let erop dat de variabele namen lokaal zijn.
function verdubbel($invoer) {
	$uitkomst = $invoer * 2;
	return $uitkomst;
}

// hoofdcode
print "Verdubbel de winst: <br>";

$winst=1000;

print "Winst = $winst == > ";
print verdubbel($winst); #let op de naam van de variabele die we meegeven







